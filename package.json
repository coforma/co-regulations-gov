{
  "author": "Coforma",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "concurrently": "4.1.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  },
  "description": "Tool for grabbing and collating comments on regulations from regulations.gov",
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "ISC",
  "name": "co-regulations-gov",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/coforma/co-regulations-gov"
  },
  "scripts": {
    "api": "yarn workspace co-regulations-gov-api start",
    "api:dev": "yarn workspace co-regulations-gov-api start:dev",
    "cli": "yarn workspace co-regulations-gov-api cli",
    "start": "yarn utils && concurrently --kill-others-on-fail \"yarn api\"  \"yarn ui\"",
    "start:dev": "yarn utils && concurrently --kill-others-on-fail \"yarn api:dev\"  \"yarn ui:dev\"",
    "ui": "yarn workspace co-regulations-gov-ui start",
    "ui:dev": "yarn workspace co-regulations-gov-ui start",
    "utils": "yarn workspace co-regulations-gov-utils build"
  },
  "version": "0.0.1",
  "workspaces": [
    "co-regulations-gov-api",
    "co-regulations-gov-ui",
    "co-regulations-gov-utils"
  ]
}
