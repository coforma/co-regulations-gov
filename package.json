{
  "author": "Coforma",
  "dependencies": {
    "concurrently": "4.1.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1"
  },
  "description": "Tool for grabbing and collating comments on regulations from regulations.gov",
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "ISC",
  "name": "co-regulations-gov",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/coforma/co-regulations-gov"
  },
  "scripts": {
    "cli": "yarn workspace co-regulations-gov-api cli",
    "client": "yarn workspace co-regulations-gov-ui start",
    "client:dev": "yarn workspace co-regulations-gov-ui start",
    "server": "yarn workspace co-regulations-gov-api start",
    "server:dev": "yarn workspace co-regulations-gov-api start:dev",
    "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\"",
    "start:dev": "concurrently --kill-others-on-fail \"yarn server:dev\"  \"yarn client:dev\""
  },
  "version": "0.0.0",
  "workspaces": [
    "co-regulations-gov-api",
    "co-regulations-gov-ui"
  ]
}
